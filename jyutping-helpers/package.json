{
  "private": true,
  "name": "jyutping-helpers",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "node ./esbuild.config.js && $(npm bin)/tsc",
