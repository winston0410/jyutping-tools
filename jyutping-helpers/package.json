  "license": "MIT",
  "devDependencies": {
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "esbuild": "0.14.21",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "prettier": "2.4.1",
    "ts-jest": "27.1.3",
    "tsc-files": "1.1.3",
    "typescript": "4.4.3"
  }
